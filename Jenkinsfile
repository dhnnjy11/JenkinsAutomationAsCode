            steps {
                echo 'This is build step'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is deploy step'
            }
        }
        stage('Test') {
            steps {
                echo 'This is test step'
            }
        }
        stage('Release') {
            steps {
                echo 'This is release step'
            }
        }
    }
}
